{
  "name": "@magicappdev/monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/magicappdev/magicappdev.git"
  },
  "license": "MIT",
  "author": {
    "name": "involvex"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:api": "turbo build --filter=@magicappdev/api",
    "build:cli": "turbo build --filter=@magicappdev/cli",
    "build:db": "turbo build --filter=@magicappdev/database",
    "build:mobile": "turbo build --filter=@magicappdev/mobile",
    "build:shared": "turbo build --filter=@magicappdev/shared",
    "build:web": "turbo build --filter=@magicappdev/web",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "check": "prettier . --write && pnpx nx run-many -t lint:fix test typecheck build  --all",
    "deploy:agent": "turbo run deploy --filter=@magicappdev/agent",
    "deploy:api": "turbo run deploy --filter=@magicappdev/api",
    "deploy:cli": "turbo run deploy --filter=@magicappdev/cli",
    "deploy:db": "turbo run deploy --filter=@magicappdev/database",
    "deploy:llm": "turbo run deploy --filter=@magicappdev/llmchat",
    "deploy:mobile": "turbo run deploy --filter=@magicappdev/mobile",
    "deploy:web": "turbo run deploy --filter=@magicappdev/web",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "nx": "nx",
    "predeploy": "pnpm build",
    "prepare": "husky",
    "sync": "nx sync",
    "sync:check": "nx sync:check",
    "typecheck": "nx run-many --target=typecheck --all"
  },
  "dependencies": {
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@conventional-changelog/git-client": "^2.5.1",
    "@expo/metro-runtime": "^6.1.2",
    "@nx/js": "22.4.1",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.10",
    "@types/node": "^25.0.10",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "knip": "^5.82.0",
    "lint-staged": "^16.2.7",
    "nx": "22.4.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.9",
    "tslib": "^2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.1",
    "wrangler": "^4.61.1"
  },
  "packageManager": "pnpm@10.28.1",
  "pnpm": {
    "overrides": {
      "@expo/metro-runtime": "^6.1.2",
      "esbuild": ">=0.25.0",
      "jsondiffpatch": ">=0.6.0",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-native-renderer": "19.1.0",
      "tar": ">=7.5.7"
    }
  }
}
