{
  "name": "@magicappdev/monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/magicappdev/magicappdev.git"
  },
  "license": "MIT",
  "author": {
    "name": "involvex"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:api": "turbo build --filter=@magicappdev/api",
    "build:cli": "turbo build --filter=@magicappdev/cli",
    "build:db": "turbo build --filter=@magicappdev/database",
    "build:mobile": "turbo build --filter=@magicappdev/mobile",
    "build:shared": "turbo build --filter=@magicappdev/shared",
    "build:web": "turbo build --filter=@magicappdev/web",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "deploy:api": "turbo run deploy --filter=@magicappdev/api",
    "deploy:cli": "turbo run deploy --filter=@magicappdev/cli",
    "deploy:db": "turbo run deploy --filter=@magicappdev/database",
    "deploy:mobile": "turbo run deploy --filter=@magicappdev/mobile",
    "deploy:web": "turbo run deploy --filter=@magicappdev/web",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "nx": "nx",
    "predeploy": "pnpm build",
    "prepare": "husky",
    "typecheck": "nx run-many --target=typecheck --all"
  },
  "dependencies": {
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "framer-motion": "^12.27.1",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.7.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@nx/js": "22.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.13.3",
    "@types/node": "^25.0.9",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "knip": "^5.82.0",
    "lint-staged": "^16.2.7",
    "nx": "22.3.3",
    "prettier": "^3.8.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.22",
    "prettier-plugin-sort-imports": "^1.8.9",
    "tslib": "^2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "packageManager": "pnpm@10.28.0"
}
