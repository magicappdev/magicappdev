{
  "name": "@magicappdev/monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/magicappdev/magicappdev.git"
  },
  "license": "MIT",
  "author": {
    "name": "involvex"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:agent": "turbo build --filter=@magicappdev/agent",
    "build:api": "turbo build --filter=@magicappdev/api",
    "build:axiom": "turbo build --filter=@magicappdev/axiom-cloudflare-workers",
    "build:cli": "turbo build --filter=@magicappdev/cli",
    "build:db": "turbo build --filter=@magicappdev/database",
    "build:llmchat": "turbo build --filter=@magicappdev/llmchat",
    "build:mobile": "turbo build --filter=@magicappdev/mobile",
    "build:shared": "turbo build --filter=@magicappdev/shared",
    "build:templates": "turbo build --filter=@magicappdev/templates",
    "build:web": "turbo build --filter=@magicappdev/web",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "check": "turbo build lint typecheck test",
    "clean": "turbo run clean && rm -rf node_modules",
    "deploy:agent": "turbo run deploy --filter=@magicappdev/agent",
    "deploy:api": "turbo run deploy --filter=@magicappdev/api",
    "deploy:axiom": "turbo run deploy --filter=@magicappdev/axiom-cloudflare-workers",
    "deploy:cli": "turbo run deploy --filter=@magicappdev/cli",
    "deploy:db": "turbo run deploy --filter=@magicappdev/database",
    "deploy:llm": "turbo run deploy --filter=@magicappdev/llmchat",
    "deploy:mobile": "turbo run deploy --filter=@magicappdev/mobile",
    "deploy:web": "turbo run deploy --filter=@magicappdev/web",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "node scripts/patch-expo-router.cjs",
    "knip": "knip",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "nx": "nx",
    "predeploy": "pnpm build",
    "prepare": "husky",
    "sync": "nx sync",
    "sync:check": "nx sync:check",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "dependencies": {
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@conventional-changelog/git-client": "^2.5.1",
    "@expo/metro-runtime": "^6.1.2",
    "@nx/js": "22.4.1",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.10",
    "@types/node": "^25.0.10",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "knip": "^5.82.0",
    "lint-staged": "^16.2.7",
    "nx": "22.4.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.9",
    "tslib": "^2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.1",
    "wrangler": "^4.61.1"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "overrides": {
      "@babel/runtime": "^7.28.6",
      "@expo/metro-runtime": "^6.1.2",
      "@types/react": "^18.3.12",
      "@types/react-dom": "^18.3.5",
      "esbuild": ">=0.25.0",
      "jsondiffpatch": ">=0.6.0",
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "react-native": "0.81.5",
      "tar": ">=7.5.7"
    }
  }
}
