{
  "name": "@magicappdev/templates",
  "version": "0.0.1",
  "description": "App templates and generators for MagicAppDev",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "import": "./dist/registry/index.js"
    },
    "./generators": {
      "types": "./dist/generators/index.d.ts",
      "import": "./dist/generators/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@magicappdev/shared": "workspace:*",
    "handlebars": "^4.7.8",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.53.1",
    "vitest": "^3.0.0"
  }
}
