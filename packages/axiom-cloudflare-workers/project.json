{
  "name": "axiom-cloudflare-workers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/axiom-cloudflare-workers/src",
  "projectType": "library",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["wrangler deploy"],
        "cwd": "packages/axiom-cloudflare-workers"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["wrangler dev"],
        "cwd": "packages/axiom-cloudflare-workers"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["eslint ."],
        "cwd": "packages/axiom-cloudflare-workers"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["eslint . --fix"],
        "cwd": "packages/axiom-cloudflare-workers"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx rimraf node_modules dist"],
        "cwd": "packages/axiom-cloudflare-workers"
      }
    }
  },
  "tags": ["type:axiom", "platform:cloudflare"]
}
