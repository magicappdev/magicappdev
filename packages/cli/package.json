    "execa": "^9.5.0",
    "open": "^11.0.0",
    "ora": "^9.1.0",
    "prompts": "^2.4.0",
    "tslib": "^2.3.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/prompts": "^2.4.0",
    "@types/ws": "^8.5.10",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1"
