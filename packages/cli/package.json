{
  "name": "@magicappdev/cli",
  "version": "0.0.8",
  "description": "CLI tool for creating and managing MagicAppDev apps",
  "keywords": [
    "cli",
    "MagicAppDev",
    "app-dev",
    "mobile",
    "react-native"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "createmagicapp": "./bin/cli.js",
    "mad": "./bin/cli.js",
    "magicappdev": "./bin/cli.js"
