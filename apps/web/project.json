{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["vite"],
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["vite build"],
        "cwd": "apps/web"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["vite preview"],
        "cwd": "apps/web"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc --noEmit"],
        "cwd": "apps/web"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["eslint ."],
        "cwd": "apps/web"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["eslint . --fix"],
        "cwd": "apps/web"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx rimraf node_modules dist"],
        "cwd": "apps/web"
      }
    }
  },
  "tags": ["type:web", "platform:browser"]
}
