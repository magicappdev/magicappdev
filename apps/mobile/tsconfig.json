{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@magicappdev/agent": ["../../packages/agent/src/index.ts"],
      "@magicappdev/api": ["../../packages/api/src/index.ts"],
      "@magicappdev/cli": ["../../packages/cli/src/index.ts"],
      "@magicappdev/database": ["../../packages/database/src/index.ts"],
      "@magicappdev/database/*": ["../../packages/database/src/*"],
      "@magicappdev/llmchat": ["../../packages/llmchat/src/index.ts"],
      "@magicappdev/shared": ["../../packages/shared/src/index.ts"],
      "@magicappdev/shared/*": ["../../packages/shared/src/*"],
      "@magicappdev/templates": ["../../packages/templates/src/index.ts"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
