{
  "name": "@magicappdev/mobile",
  "version": "0.0.3",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "android": "expo run:android",
    "build:android": "expo run:android --variant release",
    "build:apk": "cd android && .\\gradlew assembleRelease",
    "build:ios": "expo run:ios --configuration Release",
    "clean": "npx rimraf node_modules .expo",
    "clean:all": "pnpm run clean && pnpm install && nx run mobile:prebuild:clean",
    "clean:android": "cd android && .\\gradlew --stop && .\\gradlew clean --no-build-cache && npx rimraf .cxx",
    "doctor": "expo-doctor",
    "generate:icons": "node --import tsx scripts/generate-icons.ts",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "start": "expo start --clear",
    "start:reset": "expo start --reset-cache",
    "typecheck": "tsc --noEmit",
    "version:major": "npm version major --no-git-tag-version && pnpm run version:sync",
    "version:minor": "npm version minor --no-git-tag-version && pnpm run version:sync",
    "version:patch": "npm version patch --no-git-tag-version && pnpm run version:sync",
    "version:sync": "node scripts/sync-version.mjs",
    "web": "expo start --web"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@magicappdev/shared": "workspace:*",
    "@react-navigation/native": "^7.1.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~54.0.32",
    "expo-asset": "^12.0.12",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "^18.0.13",
    "expo-dev-client": "^6.0.20",
    "expo-font": "~14.0.11",
    "expo-linking": "^8.0.11",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "^29.0.16",
    "expo-web-browser": "~15.0.10",
    "invariant": "^2.2.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.16.0",
    "react-native-sse": "^1.2.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "text-encoding": "^0.7.0",
    "web-streams-polyfill": "^3.3.0",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@types/react": "19.1.17",
    "babel-preset-expo": "^54.0.10",
    "canvas": "^3.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "expo-doctor": "^1.17.14",
    "rimraf": "^6.1.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1"
  }
}
