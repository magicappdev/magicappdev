      "command": {
        "command": "run",
        "script": "build:web",
        "type": "pnpm"
      },
      "enabled": true,
      "id": "buildweb",
      "priority": 100,
      "text": "üî® Build Web",
      "tooltip": "Run build command"
    },
    {
      "alignment": "left",
      "command": {
        "command": "run",
        "script": "check",
        "type": "pnpm"
      },
      "enabled": true,
      "id": "check",
      "priority": 100,
      "text": "‚ö†Ô∏è Check",
      "tooltip": "Run check command"
    },
    {
      "alignment": "left",
      "command": {
