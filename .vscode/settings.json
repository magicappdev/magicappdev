  "statusbarQuickActions.settings.icons": {
    "library": "material"
  },
  "statusbarQuickActions.settings.performance": {
    "cacheResults": false,
    "enableVirtualization": false,
    "visibilityDebounceMs": 0
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.env.windows": {
    "ANDROID_HOME": "${env:ANDROID_HOME}",
    "JAVA_HOME": "${env:JAVA_HOME}"
  },
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "icon": "terminal-cmd",
      "path": ["${env:windir}\\System32\\cmd.exe"]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "typescript.tsc.autoDetect": "on",
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "vscode-edge-devtools.browserFlavor": "Default",
  "vscode-edge-devtools.defaultUrl": "localhost:3030",
  "vscode-edge-devtools.webRoot": "${workspaceFolder}/apps/web",
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#00ff41",
    "editorBracketHighlight.foreground2": "#00d9ff",
    "editorBracketHighlight.foreground3": "#ffa500",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
  }
}
