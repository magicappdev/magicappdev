{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "tags": ["-lintignore"],
  "workspaces": {
    "apps/*": {},
    "packages/*": {}
  },
  "pnpm": true,
  "typescript": true,
  "eslint": true,
  "prettier": true,
  "nx": true,
  "ignore": [
    "knip.json",
    "**/*.d.ts",
    "apps/web/src/components/ui/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/*-example.ts"
  ],
  "commitlint": true,
  "husky": true,
  "ignoreDependencies": [
    "@magicappdev/*",
    "tslib",
    "@radix-ui/*",
    "ai",
    "zod",
    "metro-minify-terser",
    "expo-build-properties"
  ],
  "ignoreBinaries": ["wrangler", "vitest"],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": [],
  "github-actions": true,
  "babel": true,
  "expo": true,
  "metro": true
}
